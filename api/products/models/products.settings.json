{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "name": "Products",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "category": {
      "type": "string",
      "required": true
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "description": {
      "type": "richtext",
      "required": true
    },
    "featured": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "imgUrl": {
      "type": "string"
    },
    "quantity": {
      "type": "biginteger",
      "default": "1",
      "min": "1"
    }
  }
}
